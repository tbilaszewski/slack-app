{
  "compilerOptions": {
    "experimentalDecorators": true,
    "module": "commonjs",
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "outDir": "lib",
    "sourceMap": true,
    "strict": false,
    "target": "es2017",
    "resolveJsonModule": true,
    "strictNullChecks": false,
    "lib": ["esnext.asynciterable"] 
  },
  "skipLibCheck": false,
  "typeRoots": [
    "./functions/node_modules/@types"
  ],
  "compileOnSave": true,
  "include": [
    "src"
  ],
  
}